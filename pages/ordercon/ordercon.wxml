<!--pages/ordercon/ordercon.wxml-->



<!-- navbar -->
<!-- <cover-view class='navbar-v'  style='height: {{navheight + 46}}px'>
  <navbar-v class="navbar"  name="订单详情" naviteml="1"></navbar-v>
</cover-view> -->

<!-- 头部信息 -->
<!-- <view class='header-v' style='margin-top:{{navheight + 46 + 2}}px'> -->
<view class='header-v' >
  <!-- 图片 -->
  <image class='header-img' src='../../static/images/recharge_logo.png' mode='aspectFill'></image>
  <!-- 标题 -->
  <view class='header-title'>积分交易</view>
  <view class='header-v'>
    <!-- 交易金额文字 -->
    <view class='header-con'>交易金额</view>
    <!-- 金额 -->
    <view class='header-money'>￥{{json.orderMoney}}</view>

  </view>
  
</view>

<!-- 内容信息 -->
<view class='con-v'>
  <view class='back-v'>
    <!-- 交易金额文字 -->
    <view class='con'>订单号:</view>
    <!-- 金额 -->
    <view class='money'>{{json.orderId}}</view>
  </view>
  <view class='back-v'>
    <!-- 交易金额文字 -->
    <view class='con'>交易流水号:</view>
    <!-- 金额 -->
    <view class='money'>{{json.paymentSn}}</view>
  </view>
  <view class='back-v'>
    <!-- 交易金额文字 -->
    <view class='con'>支付方式:</view>
    <!-- 金额 -->
    <view class='money'>微信支付</view>
  </view>

  <view class='lin'></view>

  <view class='back-v'>
    <!-- 交易金额文字 -->
    <view class='con'>创建时间:</view>
    <!-- 金额 -->
    <view class='money'>{{json.createTime}}</view>
  </view>
  <view class='back-v'>
    <!-- 交易金额文字 -->
    <view class='con'>付款时间:</view>
    <!-- 金额 -->
    <view class='money'>{{json.paymentTime}}</view>
  </view>
  <view class='back-v'>
    <!-- 交易金额文字 -->
    <view class='con'>完成时间:</view>
    <!-- 金额 -->
    <view class='money'>{{json.completeTime}}</view>
  </view>


</view>