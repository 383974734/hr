.list {
  border-radius: 10rpx;
  margin: 20rpx 20rpx;
  
  /* 设置阴影  ：  https://www.html.cn/tool/css3Preview/Box-Shadow.html */
-moz-box-shadow:0px 2px 3px #ececec;
-webkit-box-shadow:0px 2px 3px #ececec;
box-shadow:0px 2px 3px #ececec;

}

.list-img {
  width: 170rpx;
  height: 170rpx;
  border-radius: 85rpx;
  display: inline-block;
  vertical-align: top;
  margin-top: 30rpx;
  margin-right: 20rpx;
