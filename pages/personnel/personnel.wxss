}

.list-name {
  height: 70rpx;
  width: 75%;
  font-size: 36rpx;
  line-height: 70rpx;
  display: inline-block;
  margin-top: 15rpx;
}

.list-fork {
  width: 40rpx;
  height: 40rpx;
  float: right;
}

.list-time {
  width: 25%;
  height: 30rpx;
  float: right;
  font-size: 28rpx;
  color: #777373;
  margin-top: -30rpx;
   /* 隐藏超出部分 */
  overflow:hidden;
  /* 显示省略号 */
  text-overflow: ellipsis;
  display:-webkit-box;
  /* 块元素显示的文本行数 */
  -webkit-line-clamp:1;
  -webkit-box-orient:vertical;
}

.list-industry {
  /* mix-width: 170rpx; */
  /* min-width: 210rpx; */
  width: 240rpx;
  height: 40rpx;
  font-size: 30rpx;
  display: inline-block;
  /* margin-right: 80rpx; */
