  margin-top: 10rpx;
  overflow: hidden;
}

.list-position {
  max-width: 170rpx;
  height: 40rpx;
  font-size: 30rpx;
  display: inline-block;
  margin-bottom: 30rpx;
  margin-top: 10rpx;
  overflow: hidden;
}


.list-sub-img {
  width: 30rpx;
  height: 35rpx;
  /* 是图片和文字上对其 */
  vertical-align:top;
}

/* 电话 */
.list-tel {
  width: 210rpx;
  float: left;
  font-size: 26rpx;
  line-height: 40rpx;
}

/* 地址 */
.list-add {
  width: 180rpx;
  height: 35rpx;
  overflow: hidden;
  margin-left: 35rpx;
  float: left;
  font-size: 26rpx;
  line-height: 40rpx;
  max-width: 720rpx;
}

.list-con {
  width: 100%;
  font-size: 26rpx;
  float: left;
   /* 隐藏超出部分 */
  overflow:hidden;
  /* 显示省略号 */
  text-overflow: ellipsis;
  display:-webkit-box;
  /* 块元素显示的文本行数 */
  -webkit-line-clamp:1;
  -webkit-box-orient:vertical;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}





/* 飘起来选择的view背景 */
.zan-dialog__mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10;
  /* 设置阴影半透明背景如： background: rgba(0, 0, 0, 0.4); */
  background: rgba(0, 0, 0, 0);
  display: none;
}

/* 飘起来选择的view内容 */
.zan-dialog__container {
  position: fixed;
  top: 210rpx;
  /* bottom: 320rpx; */
  /* 弹窗布局宽 */
  width: 650rpx;
  /* 弹窗布局高，与下面弹出距离transform有关 */
  height: 280rpx;
  margin-left: 50rpx;
  background: white;
  /* 弹框弹出距离，与弹框布局高度有关，如300%表示弹起距离为3倍弹窗高度 */
  /* transform: translateY(700%); */
  transform: translateY(0);
  transition: all 0.7s ease;
  z-index: 12;
  border-radius: 20rpx;
  /* 弹框的悬浮阴影效果，如不需要可注释该行 */
  /* box-shadow: 0px 0px 3px 2px rgb(240, 240, 240); */
}

.zan-dialog--show .zan-dialog__container {
  transform: translateY(0);
}

.zan-dialog--show .zan-dialog__mask {
  display: block;
}


