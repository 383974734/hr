/* pages/enrollees/enrollees.wxss */

/* <!-- 模块 --> */

.modular-v {
  overflow: hidden;
  /* margin-top: 30rpx; */
  margin: 20rpx;
}

.modular {
  margin-top: 40rpx;
  width: 25%;
  float: left;
}

/* 图片 */
.modular-img {
  margin: 0 auto;  
  display: flex;
  justify-content: center;
  overflow: hidden;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background-color: lightgrey;
}
/* 文字 */
.modular-tit {
  width: 100%;
  height: 40rpx;
  font-size: 24rpx;
  text-align: center;
  margin-top: 10rpx;
}


/* 分类new图片显示 */
.modular-new {
  width: 55rpx;
  height: 30rpx;
  position: absolute;
  top: 3rpx;
  left: 20rpx;
}
